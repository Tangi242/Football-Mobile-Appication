{
  "expo": {
    "name": "Ballr",
    "slug": "footballhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/Logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logos/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/Logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nfa.footballhub",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/logos/Logo.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/logos/Logo.png",
          "color": "#0f172a"
        }
      ],
      "expo-sqlite"
    ]
  }
}